  "test_configurations": {
    "typo3_versions": [
      "11.5.0",
      "12.4.0",
      "13.0.0"
    ],
    "api_endpoints": {
      "ter_api": "https://extensions.typo3.org/api/v1",
      "packagist_api": "https://packagist.org/packages",
      "github_api": "https://api.github.com"
    },
    "rate_limits": {
      "github_authenticated": 5000,
      "github_unauthenticated": 60,
      "ter_api": 1000,
      "packagist_api": 100
    },
    "expected_response_times": {
      "github_api_max": 3.0,
      "ter_api_max": 5.0,
      "packagist_api_max": 3.0
    }
  },
  "mock_responses": {
    "github_repository_not_found": {
      "status_code": 404,
      "response": {
        "message": "Not Found",
        "documentation_url": "https://docs.github.com/rest"
      }
    },
    "github_rate_limit_exceeded": {
      "status_code": 403,
      "response": {
        "message": "API rate limit exceeded",
        "documentation_url": "https://docs.github.com/rest"
      }
    },
    "ter_extension_not_found": {
      "status_code": 404,
      "response": {
        "error": "Extension not found"
      }
    },
    "packagist_package_not_found": {
      "status_code": 404,
      "response": {
        "status": "error",
        "message": "Package not found"
      }
    }
  }
}

# Github API token (REST and GraphQL)
GITHUB_API_TOKEN=yourGithubAPItoken
# TYPO3 TER Access Token
TER_ACCESS_TOKEN=yourTYPO3_ExtensionRepositoryAcccesToken
# Enable/disable real API calls
ENABLE_REAL_API_CALLS=true

# Timeout settings (seconds)
INTEGRATION_TEST_TIMEOUT=30
API_REQUEST_TIMEOUT=10

# Rate limiting
API_RATE_LIMIT_DELAY=1

# Test cache directory
INTEGRATION_TEST_CACHE_DIR=var/integration-test-cache
